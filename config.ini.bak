[general]
output_directory = data/audio
check_interval = 24
max_downloads = 10

[audio]
format = mp3
bitrate = 192k
normalize_audio = True
target_level = -18.0

[player]
default_playlist = latest
keep_history = True
auto_normalize = True

[scoring]
enable_scoring = True
score_decay = 0.9
new_content_boost = 1.5

[logging]
level = INFO
file = app.log
console = True

